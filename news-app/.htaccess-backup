    RewriteEngine On
    
    RewriteRule ^storage/images/(.*)$ /home/sitr3447/public_html/be.situsintan.org/news-app/storage/app/public/images/$1 [L]

    # Arahkan semua akses ke folder public
    RewriteCond %{REQUEST_URI} !^/public/
    RewriteRule ^(.*)$ /public/$1 [L]

    # Cegah daftar isi direktori
    Options -Indexes
</IfModule>

